<h1 align= "center">Add New Address ></h1>

<center>

  <%= form_for @address, url: addresses_path do |f| %>

    <%= f.label :address_type %>
    <%= f.text_field :address_type %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :zipcode %>
    <%= f.text_field :zipcode %>

    <%= f.submit "Add New Address" %>
  <% end %>
</center>
