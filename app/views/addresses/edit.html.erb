<h1 align= "center">Edit Address %></h1>

<center>
  <%= form_for(@address) do |f| %>

    <%= f.label :address_type %>
    <%= f.text_field :address_type %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :zipcode %>
    <%= f.text_field :zipcode %>

    <%= f.submit %>
  <% end %>
</center>
